@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ourontology: <http://www.kdeproject.owl/> .

<#IctBaseTriplesMap>
         a rr:TriplesMap ; 
         rr:logicalTable [ rr:tableName "ict_area" ] ;
         rr:subjectMap [
               rr:template "http://www.kdeproject.owl/{ISO}" ;
               rr:class ourontology:ISO ;
         ] ;
         rr:predicateObjectMap [
               rr:predicate ourontology:representsCountry ;
               rr:objectMap [ rr:template "http://www.kdeproject.owl/{COUNTRY}" ] ;
         ]
         .
<#RegionTriplesMap>
         a rr:TriplesMap ; 
        rr:logicalTable [ rr:tableName "ict_area" ] ;
        rr:subjectMap [
               rr:template "http://www.kdeproject.owl/{UNICEF_REGION}" ;
               rr:class ourontology:UnicefRegion ;
         ] ;
            rr:predicateObjectMap [
        rr:predicate ourontology:hasCountry;
        rr:objectMap [ rr:template "http://www.kdeproject.owl/{COUNTRY}" ] ;
    ] ;
        rr:predicateObjectMap [
               rr:predicate ourontology:unicefRegionName;
               rr:objectMap [ rr:column "UNICEF_REGION" ] ;
         ];
     rr:predicateObjectMap [
               rr:predicate ourontology:hasDemography;
               rr:objectMap [ rr:template "http://www.kdeproject.owl/{COUNTRY}/{UNICEF_REGION}/{AREA}" ] ;
         ]  
         .

<#CountryTriplesMap>
         a rr:TriplesMap ; 
         rr:logicalTable [ rr:tableName "ict_area" ] ;
         rr:subjectMap [
               rr:template "http://www.kdeproject.owl/{COUNTRY}" ;
               rr:class ourontology:Country ;
         ] ;
         rr:predicateObjectMap [
               rr:predicate ourontology:countryName;
               rr:objectMap [rr:column "COUNTRY"];  
         ] ;

        # rr:predicateObjectMap [
        #        rr:predicate ourontology:hasRegion;
        #        rr:objectMap [ rr:template "http://www.kdeproject.owl/{REGION}" ] ;
        #  ]

         .
<#DemoGraphyTriplesMap>
         a rr:TriplesMap ; 
         rr:logicalTable [ rr:tableName "ict_area" ] ;
         rr:subjectMap [
               rr:template "http://www.kdeproject.owl/{COUNTRY}/{UNICEF_REGION}/{AREA}" ;
               rr:class ourontology:ICT_Demography ;
         ] ;
        rr:predicateObjectMap [
               rr:predicate ourontology:demographyName;
               rr:objectMap [rr:column "AREA"];  
         ] ; 
